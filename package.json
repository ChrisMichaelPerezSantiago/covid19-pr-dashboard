{
  "name": "covid19-pr-dashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "gh-publish": "vue-cli-service electron:build -p always",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/img/logos/code4pr.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.14.4",
    "apexcharts": "^3.18.1",
    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.0.0-rc.15",
    "chart.js": "2.8.0",
    "core-js": "2.6.5",
    "cross-fetch": "^3.0.4",
    "electron-icon-builder": "^1.0.2",
    "electron-updater": "^4.3.1",
    "flatpickr": "4.5.7",
    "nouislider": "13.1.4",
    "register-service-worker": "1.6.2",
    "vue": "2.6.6",
    "vue-apexcharts": "^1.5.3",
    "vue-chartjs": "3.4.2",
    "vue-clipboard2": "0.3.0",
    "vue-flatpickr-component": "8.1.2",
    "vue-router": "3.0.1",
    "vue2-transitions": "0.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-pwa": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "electron": "^6.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-electron-builder": "^1.4.6",
    "vue-template-compiler": "2.6.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
